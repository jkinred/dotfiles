[org/gnome/desktop/background]
picture-uri='file:///home/{{ .chezmoi.username }}/.local/share/backgrounds/australia-relief.png'
picture-uri-dark='file:///home/{{ .chezmoi.username }}/.local/share/backgrounds/australia-relief.png'
primary-color='#000000000000'
secondary-color='#000000000000'
picture-options='stretched'

[org/gnome/desktop/interface]
clock-format='24h'
color-scheme='prefer-dark'
enable-hot-corners=false
monospace-font-name='Hack Nerd Font Mono 16'
show-battery-percentage=true

[org/gnome/desktop/peripherals/touchpad]
natural-scroll=false
tap-to-click=true

[org/gnome/desktop/privacy]
recent-files-max-age=7
report-technical-problems=false

[org/gnome/desktop/screensaver]
picture-uri-dark='file:///home/{{ .chezmoi.username }}/.local/share/backgrounds/australia-relief.png'
primary-color='#000000000000'
secondary-color='#000000000000'

[org/gnome/desktop/search-providers]
